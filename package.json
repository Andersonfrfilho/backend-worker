{
  "name": "backend-worker",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:dev:local": "cp .env.dev.local .env && npm run start:dev",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --max-warnings 0",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\" --max-warnings 0",
    "lint:imports": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --rule 'import/order: error'",
    "format:all": "npm run format && npm run lint",
    "test:unit": "jest --config jest.config.unit.ts",
    "test:unit:watch": "jest --watch --config jest.config.unit.ts",
    "test:unit:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config jest.config.unit.ts",
    "test:unit:cov": "jest --coverage --config jest.config.unit.ts",
    "test:e2e:migration": "NODE_ENV=test npm run migration:run",
    "test:e2e": "jest --config jest.config.e2e.ts",
    "test:e2e:watch": "jest --watch --config jest.config.e2e.ts",
    "test:all": "npm run test:unit && npm run test:e2e",
    "sonar": "sonar-scanner",
    "migration:show": "node scripts/migration.js show",
    "migration:run": "node scripts/migration.js run",
    "migration:revert": "node scripts/migration.js revert",
    "migration:generate": "node scripts/migration.js generate"
  },
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/csrf-protection": "^7.1.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/swagger": "^9.5.2",
    "@fastify/swagger-ui": "^5.2.3",
    "@golevelup/nestjs-rabbitmq": "^7.1.1",
    "@nestjs-redis/kit": "^0.13.3",
    "@nestjs/cache-manager": "^3.1.0",
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-fastify": "^11.1.7",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/typeorm": "^11.0.0",
    "cache-manager-redis-yet": "^5.1.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "csurf": "^1.11.0",
    "ioredis": "^5.9.1",
    "joi": "^17.13.3",
    "mongodb": "^6.21.0",
    "nest-winston": "^1.10.2",
    "nestjs-asyncapi": "^1.4.0",
    "nestjs-redox": "^2.3.0",
    "pg": "^8.16.3",
    "redoc": "^2.5.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.27",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.9",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.2.0",
    "prettier": "^3.4.2",
    "sonarqube-scanner": "^4.3.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.20.0"
  },
  "overrides": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}